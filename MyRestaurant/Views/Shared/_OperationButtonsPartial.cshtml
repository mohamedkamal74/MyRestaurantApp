@model int
<div class="btn-group">
    <a asp-action="Details" class=" btn btn-success btn-sm text-white" asp-route-id="@Model"><i class="fa-solid fa-list-ul"></i></a>
    <a asp-action="Edit" class=" btn btn-info btn-sm text-white" asp-route-id="@Model"><i class="fa-solid fa-pen"></i></a>
    <a asp-action="Delete" class=" btn btn-danger btn-sm text-white" asp-route-id="@Model"><i class="fa-solid fa-trash-can"></i></a>
</div>
