@model SubCategory
@{
    ViewData["Title"] = "Delete";
}
<br />
<h2 class="text-info"> Delete sub Category</h2>
<br />
<form method="post" asp-action="Delete">
    <div class="whiteBackGround border">
        <input type="hidden" asp-for=Id />
        
         <div class="form-group row">
            <div class="col-2">
                <label asp-for=Category.Name class="col-form-lable"></label>
            </div>
            <div class="col-5">
                <input asp-for=Category.Name class="form-control" readonly />
             
            </div>
            </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for=Name class="col-form-lable"></label>
            </div>
            <div class="col-5">
                <input asp-for=Name class="form-control" readonly />
               
            </div>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackPartial" />
            </div>
        </div>
    </div>
  
</form>


